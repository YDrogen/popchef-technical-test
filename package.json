    "type-graphql": "^1.1.1",
    "typeorm": "^0.3.6"
  },
  "devDependencies": {
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@nrwl/cli": "14.3.6",
