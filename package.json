{
  "name": "fullstack",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "nx:run": "nx run",
    "start": "nx serve",
    "start:all": "nx run-many --target=serve --projects=frontend,backend",
    "start:backend": "nx serve backend",
    "typeorm-cli": "ts-node --project tsconfig.base.json -O '{\"module\": \"commonjs\", \"experimentalDecorators\": true}' -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:run": "npm run typeorm-cli -- migration:run -d apps/backend/src/app/services/Database.ts",
    "migration:revert": "npm run typeorm-cli -- migration:revert -d apps/backend/src/app/services/Database.ts",
    "migration:generate": "npm run typeorm-cli -- migration:generate -d apps/backend/src/app/services/Database.ts",
    "build": "nx build",
    "test": "nx test",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down --remove-orphans"
  },
  "private": true,
  "dependencies": {
